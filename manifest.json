{
  "manifest_version": 2,
  "name": "Lookup Dictionary",
  "version": "1.0",
  "description": "Type 'dict' with a search word to lookup online dictionary.",
  "author": "anthonychen",
  "permissions": ["contextMenus"],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "omnibox": {
    "keyword": "dict"
  },
  "browser_action": {
    "default_icon": {
      "64": "icons8-google-translate-64.png"
    }
  },
  "icons": {
    "16": "icons8-google-translate-16.png",
    "32": "icons8-google-translate-32.png",
    "64": "icons8-google-translate-64.png",
    "128": "icons8-google-translate-128.png"
  }
}
